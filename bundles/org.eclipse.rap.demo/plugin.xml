<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension
      id="org.eclipse.rap.demo.demoentrypoint"
      point="org.eclipse.rap.ui.workbench.entrypoint">
    <entrypoint
        class="org.eclipse.rap.demo.DemoWorkbench"
        parameter="default"/>
    <entrypoint
        class="org.eclipse.rap.demo.EntryPoint"
        parameter="rwtdemo"/>
    <entrypoint
        class="org.eclipse.rap.demo.controls.LayoutDemo"
        parameter="layout"/>
    <entrypoint
        class="org.eclipse.rap.demo.controls.ControlsDemo"
        parameter="controls"/>
  </extension>
  
  <extension
      point="org.eclipse.rap.ui.workbench.perspectives">
    <perspective
        name="Demo Perspective"
        class="org.eclipse.rap.demo.Perspective"
        id="org.eclipse.rap.demo.perspective">
    </perspective>
  </extension>
  
  <extension point="org.eclipse.rap.ui.workbench.views"> 
      <!-- category 
